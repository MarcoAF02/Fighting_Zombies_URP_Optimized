				float2 texCoord39 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 appendResult31 = (float2(-_FlameFlickerSpeed , -_FlameFlickerSpeed));
				float2 appendResult47 = (float2(WorldPosition.x , WorldPosition.y));
				float2 texCoord30 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner29 = ( 0.37 * _Time.y * appendResult31 + ( ( appendResult47 * 0.1 ) + ( _NoiseScale * texCoord30 ) ));
				float4 tex2DNode1 = tex2D( _Textures, ( texCoord39 + ( texCoord39.y * ( (tex2D( _Noise, panner29 )).rg - float2( 0,0 ) ) * texCoord39.x * texCoord39.y ) ) );
				float2 uv_TextureSample0 = IN.ase_texcoord3.xy * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
				float4 tex2DNode51 = tex2D( _TextureSample0, uv_TextureSample0 );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
